{
	"languageserver": {
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp","cs","py"],
			"rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/","ProjectSettings/"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				}
			},
		}
	},
	"coc.source.OmniSharp.enable" : true,
	"coc.source.OmniSharp.triggerCharacters": "."
}
